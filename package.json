{
  "name": "story-frontend",
  "version": "0.0.0",
  "description":
    "Frontend for the Story application. React based SPA (Single-Page Application)",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/story-ai/frontend."
  },
  "keywords": ["react", "typescript"],
  "scripts": {
    "start":
      "node_modules\\.bin\\concurrently \"node dev-server.js\" \"npm run watch\"",
    "build": "node build.js",
    "test":
      "mocha-webpack \"src/**/*.test.{js,jsx,ts,tsx}\" --webpack-config webpack.config.test.js",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "tslint": "tslint -c ./tslint.json \"src/**/*.{ts,tsx}\"",
    "lint-fix": "npm run lint -- --fix",
    "tslint-fix": "npm run tslint -- --fix",
    "watch": "npm-watch",
    "deploy":
      "aws --profile serverless --region eu-west-1 s3 sync dist s3://frontend.story.ai"
  },
  "watch": {
    "test": {
      "patterns": ["src/**/*"],
      "extensions": "js,jsx,ts,tsx"
    }
  },
  "dependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.76.0",
    "underscore": "^1.8.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/core-js": "0.9.35",
    "@types/deep-freeze": "0.0.29",
    "@types/mocha": "2.2.39",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.24",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.13",
    "@types/react-router": "^4.0.17",
    "@types/react-router-dom": "^4.2.1",
    "@types/redux-form": "^7.0.9",
    "@types/redux-logger": "^3.0.5",
    "@types/underscore": "^1.7.36",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chai": "3.5.0",
    "clean-webpack-plugin": "0.1.15",
    "colors": "1.1.2",
    "concurrently": "2.2.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "deep-freeze": "0.0.1",
    "eslint": "3.16.1",
    "eslint-plugin-react": "6.10.0",
    "eslint-teamcity": "1.3.1",
    "expose-loader": "0.7.3",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.28.0",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "mocha-teamcity-reporter": "1.1.1",
    "mocha-webpack": "1.0.0-beta.1",
    "node-sass": "3.13.1",
    "npm-watch": "0.1.8",
    "null-loader": "0.1.1",
    "react-addons-test-utils": "15.3.0",
    "react-hot-loader": "3.0.0-beta.1",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "ts-loader": "^3.1.1",
    "tslint": "4.5.1",
    "tslint-eslint-rules": "3.4.0",
    "tslint-teamcity-reporter": "2.0.0",
    "typescript": "^2.6.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.3",
    "webpack-node-externals": "1.5.4"
  }
}
