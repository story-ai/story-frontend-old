{
  "name": "story-frontend",
  "version": "0.2.0",
  "description": "Prototype Frontend for the Story platform",
  "main": "./dist/bundle.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.ts",
    "start": "webpack-dev-server --config webpack.config.dev.ts",
    "prod": "static dist",
    "deploy":
      "aws --profile story-serverless --region eu-west-2 s3 sync --delete dist s3://frontend.story-ai.com && npm run invalidate",
	  "invalidate": "aws cloudfront --profile story-serverless create-invalidation --distribution-id EJJ8POR7G55LE --paths '/*'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
