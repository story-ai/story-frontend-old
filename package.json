{
  "name": "story-frontend",
  "version": "0.2.0",
  "description": "Prototype Frontend for the Story platform",
  "main": "./dist/bundle.js",
  "env": {
    "prod": {
      "webpack": "webpack.config.prod.ts",
      "s3": "s3://frontend.story-ai.com",
      "cloudfront": "EJJ8POR7G55LE"
    },
    "staging": {
      "webpack": "webpack.config.staging.ts",
      "s3": "s3://stage.frontend.story-ai.com",
      "cloudfront": "EF0JUCH1WEGGB"
    },
    "dev": {
      "webpack": "webpack.config.dev.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config $npm_package_env_dev_webpack",
    "prod-start": "static dist",
    "staging-start": "static dist",
    "build": "webpack",
    "prod-build": "npm run build -- --config $npm_package_env_prod_webpack",
    "staging-build":
      "npm run build -- --config $npm_package_env_staging_webpack",
    "deploy":
      "aws --profile story-serverless --region eu-west-2 s3 sync --delete dist",
    "prod-deploy":
      "npm run deploy -- $npm_package_env_prod_s3 && npm run prod-invalidate",
    "staging-deploy":
      "npm run deploy -- $npm_package_env_staging_s3 && npm run staging-invalidate",
    "invalidate":
      "aws cloudfront --profile story-serverless create-invalidation --paths '/*'",
    "prod-invalidate":
      "npm run invalidate -- --distribution-id  $npm_package_env_prod_cloudfront",
    "staging-invalidate":
      "npm run invalidate -- --distribution-id  $npm_package_env_staging_cloudfront",
    "generate-string-schema":
      "i18n-tag-schema ./src --preprocessor ./preprocessors/typescript --filter \"\\.[tj]sx?\" --schema ./translation.schema.json",
    "validate-string-schema":
      "npm run generate-string-schema && i18n-tag-schema ./src/strings --validate --schema ./translation.schema.json"
  },
  "dependencies": {
    "@types/aws-lambda": "0.0.27",
    "@types/aws-sdk": "^2.7.0",
    "aws-sdk": "^2.188.0",
    "axios": "^0.17.1",
    "bignumber": "^1.1.0",
    "bluebird": "^3.5.1",
    "es2015-i18n-tag": "^1.3.1",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^3.8.2",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-intercom": "^1.0.14",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scroll": "^1.7.7",
    "react-stripe-checkout": "^2.6.3",
    "react-visibility-sensor": "^3.11.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.2",

    "story-backend-utils": "^0.1.10",
    "superagent": "^3.8.2",
    "underscore": "^1.8.3",
    "whatwg-fetch": "^2.0.3",
    "yaml-loader": "^0.5.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/clean-webpack-plugin": "^0.1.0",
    "@types/es6-promise": "^3.3.0",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/immutable": "^3.8.7",
    "@types/js-yaml": "^3.10.1",
    "@types/lodash": "^4.14.101",
    "@types/node": "^9.4.0",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-helmet": "^5.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.13",
    "@types/react-router": "^4.0.21",
    "@types/react-router-dom": "^4.2.1",
    "@types/react-scroll": "^1.5.3",
    "@types/redux-form": "^7.0.13",
    "@types/redux-immutable": "^3.0.38",
    "@types/redux-logger": "^3.0.5",
    "@types/superagent": "^3.5.7",
    "@types/underscore": "^1.7.36",
    "@types/webpack": "^3.8.5",
    "@types/webpack-env": "^1.13.5",
